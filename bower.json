{
  "name": "paper-elements",
  "version": "2.0.0",
  "homepage": "https://github.com/PolymerElements/paper-elements",
  "authors": [
    "The Polymer Authors"
  ],
  "description": "Paper elements are a set of visual elements that implement Google's Material Design.",
  "keywords": [
    "web-components",
    "polymer",
    "paper",
    "element-collection"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/PolymerElements/paper-elements.git"
  },
  "license": "http://polymer.github.io/LICENSE.txt",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "paper-input-elements": "PolymerElements/paper-input-elements#2.0",
    "paper-overlay-elements": "PolymerElements/paper-overlay-elements#2.0",
    "paper-ui-elements": "PolymerElements/paper-ui-elements#2.0",
    "paper-behaviors": "PolymerElements/paper-behaviors#2.0",
    "paper-ripple": "PolymerElements/paper-ripple#2.0",
    "paper-styles": "PolymerElements/paper-styles#2.0"
  },
  "main": "paper-elements.html",
  "variants": {
    "1.x": {
      "dependencies": {
        "paper-input-elements": "PolymerElements/paper-input-elements#*",
        "paper-overlay-elements": "PolymerElements/paper-overlay-elements#*",
        "paper-ui-elements": "PolymerElements/paper-ui-elements#*",
        "paper-behaviors": "PolymerElements/paper-behaviors#*",
        "paper-ripple": "PolymerElements/paper-ripple#*",
        "paper-styles": "PolymerElements/paper-styles#*"
      }
